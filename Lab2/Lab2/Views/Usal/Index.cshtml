@model Lab2.Models.DoctorHospital

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>@Model.Hospital.Name</h1>
<h1>@Model.Hospital.Address</h1>

<table class="table">
    <thead>
        <tr>
            <th>Doctor Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var doc in Model.Doctor)
        {
            <tr>
                <td><a href="/Usal/DocInfo/@doc.Name">@doc.Name</a></td>
            </tr>
        }
    </tbody>
</table>


